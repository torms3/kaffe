[forward]
kaffe_root  = /path/to/kaffe/root
dspec_path  = %(kaffe_root)s/data_spec/dataset.spec
model       = /path/to/prototxt
weights     = /path/to/train_iter_%(witer)s.caffemodel.h5
wrange      = [100000,200000,300000,...]
drange      = [0,1,2,...]
fov         = (z,y,x)
border      = dict(type='mirror_border', fov=%(fov)s)
scan_params = None
save_prefix = /path/to/save/iter_%(witer)s/prefix
# Inference-time augmentation.
scan_list   = {'output':dict(dst=(1,1,1))}
